<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emakina.iclu.generalapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_chicken"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_chicken"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SmallBirdActivity" />
        <activity android:name=".activities.BirdStoryActivity" />
        <activity android:name=".activities.MediumBirdActivity" />
        <activity android:name=".activities.BigBirdActivity" />
        <activity android:name=".activities.AboutActivity"></activity>
    </application>

</manifest>